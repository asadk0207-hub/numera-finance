{
  "trailingSlash": false,
  "cleanUrls": true,
  "public": false,
  "github": {
    "autoDeployments": true
  },
  "rewrites": [
    {
      "source": "/ads.txt",
      "destination": "/docs/ads.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/docs/$1"
    }
  ],
  "headers": [
    {
      "source": "/ads.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}
